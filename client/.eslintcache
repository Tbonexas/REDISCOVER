tch();\n\n    // populates the values of the form with a callback and a dependency array // \n   useEffect(() => {\n       if(post) {\n           console.log(post)\n           setPostData(post);\n       }  \n   }, [post])\n\n    // when user hit submit it will send a post request with the data they added // \n    const handleSubmit = (e) => {\n        \n        e.preventDefault();\n\n        if(currentId) {\n            dispatch(updatePost(currentId, postData));\n        } else {\n            dispatch(createPost(postData));\n        // regardless of if or else we will clear the form and call the clear function at the end //     \n        }\n        clear();\n    }\n// This function will clear form after submit is click upon editing a post // \n    const clear = () => {\n        setCurrentId(null);\n        setPostData({creator: '', title: '', message: '', tags: '', selectedFile: ''});\n    }\n\n    return(\n        <Paper className={classes.paper}>\n        <form autoComplete=\"off\" no noValidate className={`${classes.root} ${classes.form}`} onSubmit={handleSubmit}>\n        <Typography varient='h6'>Have a  place to share? Upload it below:</Typography>\n        <TextField name=\"creator\" variant=\"outlined\" label=\"Creator\" fullWidth value={postData.creator} onChange={(e) => setPostData({ ...postData, creator: e.target.value })}/>\n\n        <TextField name=\"title\" variant=\"outlined\" label=\"Title\" fullWidth value={postData.title} onChange={(e) => setPostData({ ...postData, title: e.target.value })}/>\n\n        <TextField name=\"message\" variant=\"outlined\" label=\"Message\" fullWidth value={postData.message} onChange={(e) => setPostData({ ...postData, message: e.target.value })}/>\n        <TextField name=\"tags\" variant=\"outlined\" label=\"Tags\" fullWidth value={postData.tags} onChange={(e) => setPostData({ ...postData, tags: e.target.value })}/>\n        <div className={classes.fileInput}><FileBase type=\"file\" multiple={false} onDone={({base64}) => setPostData({...postData, selectedFile: base64})} />\n        <Button className={classes.buttonSubmit} variant=\"contained\" color=\"primary\" size=\"large\" type=\"submit\" fullWidth>Upload</Button>\n        <Button variant=\"contained\" color=\"secondary\" size=\"small\" onClick={clear} fullWidth>Remove</Button>\n        </div>\n        </form>\n\n        </Paper>\n    );\n}\n\nexport default Form;","/Users/Chris/projects/finalProject/finalProject/client/src/components/Posts/Post/PublicPost.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/components/Posts/Post/Post.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/api/index.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/components/Form/styles.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/components/Posts/Post/styles.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/components/Auth/icon.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/reducers/auth.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/constants/actionTypes.js",[],{"ruleId":"170","replacedBy":"171"},{"ruleId":"172","replacedBy":"173"},{"ruleId":"170","replacedBy":"174"},{"ruleId":"172","replacedBy":"175"},{"ruleId":"176","severity":1,"message":"177","line":1,"column":17,"nodeType":"178","messageId":"179","endLine":1,"endColumn":25},{"ruleId":"180","severity":1,"message":"181","line":3,"column":1,"nodeType":"182","endLine":23,"endColumn":2},{"ruleId":"176","severity":1,"message":"183","line":1,"column":26,"nodeType":"178","messageId":"179","endLine":1,"endColumn":35},{"ruleId":"176","severity":1,"message":"184","line":2,"column":48,"nodeType":"178","messageId":"179","endLine":2,"endColumn":56},"no-native-reassign",["185"],"no-negated-in-lhs",["186"],["185"],["186"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign arrow function to a variable before 