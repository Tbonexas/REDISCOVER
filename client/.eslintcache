nt/src/pages/ExplorePage/Explore.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/pages/Misc/Misc.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/pages/Nature/Nature.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/pages/Film/Film.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/components/darkMode/darkMode.js",["166"],"/Users/Chris/projects/finalProject/finalProject/client/src/pages/IndexPage/Index.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/components/Auth/Auth.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/reducers/posts.js",["167"],"import { FETCH_ALL, CREATE, UPDATE, DELETE, FETCH } from \"../constants/actionTypes\";\n\nexport default (posts = [], action) => {\n   switch (action.type) {\n            // return all posts but filter out the deleted post then dispatch the action in components//\n        case FETCH_ALL:\n           return action.payload;\n        case DELETE:\n              return posts.filter((post) => post._id !== action.payload)\n            // action.payload will be the updated post otherwise just return the post without updates //\n        case UPDATE:\n           // both update and like cases can do the same thing if not they will return the original post //  \n            return posts.map((post) => post._id === action.payload._id ? action.payload : post);\n        case CREATE:\n            return [...posts, action.payload];\n        case FETCH:\n              return action.payload;\n\n\n       default:\n           return posts;\n   }\n}","/Users/Chris/projects/finalProject/finalProject/client/src/Uploads.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/pages/Historical/RenderHistorical.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/pages/Haunted/RenderHaunted.js",[],"/Users/Chris/projects/finalProject/finalProject/client/src/pages/Nature/RenderNature.js",[],"/Users/Chris/projects/finalProjec